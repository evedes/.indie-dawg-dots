[user]
	name = Eduardo Vedes
	email = eduardo.vedes@gmail.com
[core]
	editor = nvim
	excludesfile = /Users/edo/.gitignore
[pull]
	rebase = true
[push]
	autoSetupRemote = true
[init]
	defaultBranch = main
[alias]
	fixup = !git log -n 50 --pretty=format:'%h %s' --no-merges | fzf | cut -c -7 | xargs -o git commit --fixup
	gll = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
